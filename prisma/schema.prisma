generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String   @id @default(uuid())
  discordId     String   @unique
  username      String
  createdAt     DateTime @default(now())
  level         Int      @default(0)
  xp            Float    @default(0)
}